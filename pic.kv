<LabelB@Label>:
    bcolor: 1, 1, 1, 1
    canvas.before:
        Color:
            rgba: self.bcolor
        Rectangle:
            pos: self.pos
            size: self.size

<Menu>:
    cols: 1

    Button:
        size_hint_y: .07
        text: 'Interactive Python'

    BoxLayout:
        LabelB:
            bcolor: 0, 0, 1, 0.2
            size_hint_x: .2
            text: 'member list'

        Label:
            id: output
            text: ''

        LabelB:
            bcolor: 0, 0, 1, 0.2
            size_hint_x: .2
            text: 'channel list'

    BoxLayout:
        size_hint_y: .05
        Label:
            size_hint_x: .14
            text: 'emryscass'
        TextInput:
            id: chat
            size_hint_x: .86
            multiline: False
            on_text_validate: root.pass_text(self.text)
